<p id="notice"><%= notice %></p>

<p>
  <strong>User:</strong>
  <%= @users.find{|user| user.id == @order.user_id}.name %>
</p>

<p>
  <strong>Start time:</strong>
  <%= @order.start_time %>
</p>

<p>
  <strong>End time:</strong>
  <%= @order.end_time %>
</p>

<h2>Order Items</h2>
<table>
  <%= render(@order.line_items) %>
</table>

<%= link_to 'Edit', edit_order_path(@order) %> |
<%= link_to 'Back', orders_path %>
